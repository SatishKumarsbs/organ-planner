#%RAML 1.0 DataType

type: object
properties:
  request_id?: integer
  user_id: integer
  hospital_id: integer
  emergency_contact_number?: number
  emergency_contact_address?: string
  organ_name: string
  transplant_priority:
    enum: ["critical","high","medium","low"]
  survival_time: string
  medical_report?: string
  transformation_required: boolean
  status: 
    enum: ["open", "close","archieve"]
    